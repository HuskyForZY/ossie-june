				<preferred-widget type="slider"/>
				</preferred-widgets>
		</property>
		<property xmltype="simple" datatype="ushort">
			<preferred-widgets>
				<preferred-widget type="spin" default="true"/>
				<preferred-widget type="text"/>
				<preferred-widget type="slider"/>
				</preferred-widgets>
		</property>
		<property xmltype="simple" datatype="char">
			<preferred-widgets>
				<preferred-widget type="text" default="true"/>
			</preferred-widgets>
		</property>
		<property xmltype="simple" datatype="string">
			<preferred-widgets>
				<preferred-widget type="text" default="true"/>
			</preferred-widgets>
		</property>
		<property xmltype="simple" datatype="boolean">
			<preferred-widgets>
				<preferred-widget type="text" default="true"/>
			</preferred-widgets>
		</property>
		<property xmltype="simplesequence" datatype="float">
			<preferred-widgets>
				<preferred-widget type="text" default="true"/>
			</preferred-widgets>
		</property>
		<property xmltype="simplesequence" datatype="string">
			<preferred-widgets>
				<preferred-widget type="text" default="true"/>
			</preferred-widgets>		
		</property>
		<property xmltype="simplesequence" datatype="short">
			<preferred-widgets>
				<preferred-widget type="text" default="true"/>
			</preferred-widgets>		
		</property>

	</properties>
